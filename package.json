{
  "name": "pop-pyramid",
  "version": "1.0.0",
  "description": "This application is used for visual comparison of two population pyramid data sets ",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "test": "electron --inspect=9001 .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Demografie App\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "author": "Pavel Mircea",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.1.3",
    "dom-to-image": "^2.6.0",
    "downloadjs": "^1.4.7",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.6"
  },
  "devDependencies": {
    "electron": "^3.0.11",
    "electron-packager": "^13.0.1"
  }
}
